{
  "protocol": {
    "name": "Haltech CAN Broadcast Protocol",
    "version": "2.35.0",
    "bus_speed_bps": 1000000,
    "id_bits": 11,
    "endianness": "big",
    "notes": [
      "Byte 0 is first byte, byte 7 is last",
      "Bit 7 is MSB, bit 0 is LSB within a byte",
      "Temperatures are in Kelvin (subtract 273.15 for Celsius)",
      "Pressures with -101.3 offset are gauge pressure (subtract atmospheric)"
    ]
  },
  "frames": {
    "0x360": {
      "name": "Engine Core 1",
      "rate_hz": 50,
      "channels": [
        {
          "name": "RPM",
          "bytes": [0, 1],
          "signed": false,
          "units": "RPM",
          "conversion": "x",
          "description": "Engine speed"
        },
        {
          "name": "Manifold Pressure",
          "bytes": [2, 3],
          "signed": false,
          "units": "kPa",
          "conversion": "x / 10",
          "description": "Manifold absolute pressure"
        },
        {
          "name": "Throttle Position",
          "bytes": [4, 5],
          "signed": false,
          "units": "%",
          "conversion": "x / 10",
          "description": "Throttle position sensor"
        },
        {
          "name": "Coolant Pressure",
          "bytes": [6, 7],
          "signed": false,
          "units": "kPa",
          "conversion": "(x / 10) - 101.3",
          "description": "Coolant pressure (gauge)"
        }
      ]
    },
    "0x361": {
      "name": "Pressures",
      "rate_hz": 50,
      "channels": [
        {
          "name": "Fuel Pressure",
          "bytes": [0, 1],
          "signed": false,
          "units": "kPa",
          "conversion": "(x / 10) - 101.3",
          "description": "Fuel pressure (gauge)"
        },
        {
          "name": "Oil Pressure",
          "bytes": [2, 3],
          "signed": false,
          "units": "kPa",
          "conversion": "(x / 10) - 101.3",
          "description": "Oil pressure (gauge)"
        },
        {
          "name": "Engine Demand",
          "bytes": [4, 5],
          "signed": false,
          "units": "%",
          "conversion": "x / 10",
          "description": "Engine demand / accelerator pedal position"
        },
        {
          "name": "Wastegate Pressure",
          "bytes": [6, 7],
          "signed": false,
          "units": "kPa",
          "conversion": "(x / 10) - 101.3",
          "description": "Wastegate pressure (gauge)"
        }
      ]
    },
    "0x362": {
