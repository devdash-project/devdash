{
    "$schema": "../protocols/profile-schema.json",
    "name": "Example Automatic Transmission",
    "description": "Example profile demonstrating automatic transmission gear mapping (P/R/N/D/S/L)",
    "adapter": "haltech",
    "adapterConfig": {
        "interface": "can0",
        "protocolFile": "../protocols/haltech/haltech-can-protocol-v2.35.json"
    },
    "display": {
        "cluster": {
            "enabled": true,
            "screen": 0,
            "layout": "default"
        },
        "headunit": {
            "enabled": true,
            "screen": 1,
            "layout": "default"
        }
    },
    "units": {
        "temperature": "celsius",
        "pressure": "kpa",
        "speed": "kmh"
    },
    "warnings": {
        "coolantTemperature": {
            "warning": 95,
            "critical": 105
        },
        "oilPressure": {
            "warning": 150,
            "critical": 100
        }
    },
    "channelMappings": {
        "RPM": "rpm",
        "Throttle Position": "throttlePosition",
        "Engine Coolant Temperature": "coolantTemperature",
        "Battery Voltage": "batteryVoltage",
        "Vehicle Speed": "vehicleSpeed",
        "Gear Position": "gear"
    },
    "gearMapping": {
        "-2": "P",
        "-1": "R",
        "0": "N",
        "1": "D",
        "2": "S",
        "3": "L"
    }
}
